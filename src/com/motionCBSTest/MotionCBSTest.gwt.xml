<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN"
        "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="MotionCBSTest">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.googlecode.gwt.charts.Charts"/>
    <inherits name="com.googlecode.gwt.charts.Charts"/>
    <inherits name="com.googlecode.gwt.charts.Charts"/>
    <inherits name="com.googlecode.gwt.charts.Charts"/>
    <inherits name="com.googlecode.gwt.charts.Charts"/>
    <inherits name="com.googlecode.gwt.charts.Charts"/>
    <inherits name="com.googlecode.gwt.charts.Charts"/>
    <inherits name="com.googlecode.gwt.charts.Charts"/>


    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.motionCBSTest.client.MotionCBSTest'/>

    <source path="client"></source>
    <source path="shared"></source>

    <!-- Specify the app servlets.                   -->
    <servlet path='/MotionCBSTestService' class='com.motionCBSTest.server.MotionCBSTestServiceImpl'/>


</module>